<POLICIES xmlns="http://www.w3.org/2002/01/P3Pv1">
<POLICY name="policy1" 
	discuri="http://www.example.com/">
	<ENTITY>
		<DATA-GROUP>
			<DATA ref="#business.name">null</DATA>
			<DATA ref="#business.department"></DATA>
			<DATA ref="#business.contact-info.postal.street">null</DATA>
			<DATA ref="#business.contact-info.postal.city">null</DATA>
			<DATA ref="#business.contact-info.postal.stateprov">null</DATA>
			<DATA ref="#business.contact-info.postal.postalcode">000-0000</DATA>
			<DATA ref="#business.contact-info.telecom.telephone.intcode">000</DATA>
			<DATA ref="#business.contact-info.telecom.telephone.loccode">000</DATA>
			<DATA ref="#business.contact-info.telecom.telephone.number">0000</DATA>
			<DATA ref="#business.contact-info.telecom.fax.intcode">000</DATA>
			<DATA ref="#business.contact-info.telecom.fax.loccode">000</DATA>
			<DATA ref="#business.contact-info.telecom.fax.number">0000</DATA>
			<DATA ref="#business.contact-info.online.email">example@example.com</DATA>
		</DATA-GROUP>
	</ENTITY>
	<ACCESS><nonident/></ACCESS>
	<DISPUTES-GROUP>
		<DISPUTES resolution-type="service" 
			service="http://www.example.com/"
		>
		<IMG src="http://www.example.com/" alt="null"/>
		<REMEDIES><correct/></REMEDIES>
		</DISPUTES>
	</DISPUTES-GROUP>
	<STATEMENT>
		<PURPOSE>
			<current required="always" />
		</PURPOSE>
		<RECIPIENT>
			<ours></ours>
		</RECIPIENT>
		<RETENTION>
			<no-retention/>
		</RETENTION>
		<DATA-GROUP>
			<DATA ref="#dynamic.cookies" optional="yes">
				<CATEGORIES>
					<state/>
				</CATEGORIES>
			</DATA>
		</DATA-GROUP>
	</STATEMENT>
</POLICY>
</POLICIES>
